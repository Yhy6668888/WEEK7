import static java.lang.Math.max;

public class calss1 {
    public static void main(String[] args) {
        int i=5;
        int j=2;
        int k= max(i,j);
        System.out.println("The max number is "+k);
    }
    public static int max(int a, int b) {
        int result;
        if(a>b){
            result = a;
        }
        else result = b;
        return result;
    }
}
